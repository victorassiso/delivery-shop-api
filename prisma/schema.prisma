generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model User {
  id            String @id @default(uuid())
  name          String
  email         String @unique
  password_hash String
  role          Role

  created_at DateTime @default(now())

  @@map("users")
}

enum Role {
  user
  admin
}
